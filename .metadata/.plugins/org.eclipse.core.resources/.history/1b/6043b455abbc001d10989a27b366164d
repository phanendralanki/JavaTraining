	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		 int t = in.nextInt();
	        int c = in.nextInt();
	        int r = in.nextInt();
	        if((0<t&&t<=10000)&&(0<c&&c<=10000)&&(0<r&&r<=1000))
	            System.out.println("yes");
	        else
	            System.out.println("No");
	}
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
