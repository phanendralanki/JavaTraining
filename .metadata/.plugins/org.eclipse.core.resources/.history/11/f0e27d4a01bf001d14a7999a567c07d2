package com.oops;

import java.util.Scanner;

public class Practice {
	public static void main(String args[]) {
	        Scanner in = new Scanner(System.in);
	        int N = in.nextInt();
	        if(N<0)
	        {
	            System.out.println("no negative numbers");
	        }
	        else if(N%3==0){
	        	System.out.println("true");
	 }else {
		 		System.out.println("false");
	 		}
	    }

}
