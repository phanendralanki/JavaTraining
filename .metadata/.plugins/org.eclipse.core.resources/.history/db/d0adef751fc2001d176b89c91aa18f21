package com.Abstraction;

public abstract class Abstract {
	public static void leaveRequest() {
		System.out.println("leave for one day");
	}
	public static void leaveDate() {
		System.out.println("15/3/2023");
	}
	abstract static void Reason();
	
	public static void main(String[] args) {
		leaveRequest();
		leaveDate();
		Reason();
	}
}

class Another{
	public static void Reason() {
		System.out.println("Abstract Method");
	}
}
