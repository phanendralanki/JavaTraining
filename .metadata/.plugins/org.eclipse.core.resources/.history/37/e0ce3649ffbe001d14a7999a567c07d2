package com.oops;

import java.util.Scanner;

public class Practice {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		Float n = sc.nextFloat();
		Float sum = 0F;
		while (n != 0) {
            sum = sum + n % 10;
            n = n / 10;
        }
		System.out.println(sum);
	}
}
