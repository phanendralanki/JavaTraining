package com.practice;

import java.util.Scanner;

public class Fibonacci {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		//0,1,1,2,3,5,8,13
		int n1 = 0;
		int n2 = 1;
		int n3;
		int n = sc.nextInt();
		for(int i=1;i<=n;i++) {
			System.out.print(" "+n1+" "+n2+" ");
			n3 = n1+n2;
			System.out.print(n3);
			n1 = n2;
			n2 = n3;
		}
		sc.close();
	}
}
