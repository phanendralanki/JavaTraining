package com.oops;

import java.util.Scanner;

public class Practice {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
        if(n<0)
        {
            System.out.println("no negative numbers");
        }
        else{
            if(n%3==0)
            {
                System.out.println("true");
            }
            else
            {
                System.out.println("false");
            }
   	}

	}
}

