		//p.laptop();
		//p.phone();
		//p.bike();
		
		Child c = new Child();
		c.bike();
		c.phone();
		c.house();
		//c.car();
		c.laptop();
		
		System.out.println(c.bankBalance);
		System.out.println(c.cash);
		//System.out.println(c.withdraw);
		System.out.println(c.personalExpense);
		 c.Parent();
		c.Child();
	}
}
